    <section>
        <h3 id="imageUpload">Image</label>
        <input aria-labelledby="imageUpload" name="image" type="file" accept="image/*" />
    </section>
    <section>
        <input type="text" name="user.yourName" id="useryourName" value="{{formData.user.yourName}}" placeholder="Your name" aria-label="Your name" />
        <input type="text" name="user.childName" id="userchildName" value="{{formData.user.childName}}" placeholder="Child's name" aria-label="Child's name" />
        <input type="number" name="user.childAge" id="userchildAge" value="{{formData.user.childAge}}" placeholder="Child's age" aria-label="Child's age" />
        <input type="email" name="user.email" id="useremail" value="{{formData.user.email}}" placeholder="Your email address" aria-label="Your email address" />  
    </section>
    <fieldset>
        <legend>Future updates</legend>
        <ul class="wpfk--form__checkbox-container">
            <li><input type="checkbox" name="user.agreement" id="userAgreement" {{#if formData.user.agreement}}checked="checked"{{/if}} /> <label for="userAgreement">Please tick to get future updates from WPFK</label></li>
        </ul>
    </fieldset>
    <section class="wpfk--form-actions">
        <input type="hidden" name="action" value="venue">
        <button type="submit" class="wpfk--form__submit-button">Submit your wiki place</button>
    </section>
</form>
